#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>

bool isHappy(int n) {
	int dig;
	if (0 == n)
	{
		return false;
	}
	while (1!=n)
	{
		dig = 0;
		while(n != 0)
	}
}

int main(void) {
	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}
